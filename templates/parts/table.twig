
<table class="table">
  <thead><nav class="navbar navbar-light ">
  <div class="container-fluid">
    <form class="d-flex">
      <input class="form-control me-2 w-50" id="searchinput" type="search" placeholder="Search" aria-label="Search">
      <button class="btn btn-outline-success" type="submit">Search</button>
    </form>
  </div>
</nav>
    <tr>

      <th scope="col">ID</th>
      <th scope="col">Имя</th>
      <th scope="col">Фамилия</th>
      <th scope="col">Email</th>
      <th scope="col">Пол</th>
      <th scope="col">Группа</th>
      <th scope="col"><i class="fas fa-angle-down"></i> Баллы ЕГЭ</th>


    </tr>
  </thead>
  <tbody id="tb">
  {% for item in students %}
    <tr>
      
      <td>{{ item.id }}</td>
      <td>{{item.name}}</td>
      <td>{{item.surname}}</td>
      <td>{{item.email}}</td>
      <td>{{item.sex}}</td>
      <td>{{item.groupnum}}</td>
      <td>{{item.egescore}}</td>

    </tr>
  {% endfor %}
   
  </tbody>
</table>
<script type="text/javascript" src="/../public/scripts/tablescripts.js"></script>